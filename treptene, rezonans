>> y=dsolve('D2y+9*y=sin(3*t), y(0)=2, Dy(0)=-1')
tt=0:0.05:200;
yy=subs(y, 't', tt);
plot(tt, yy)
 
y =
 
2*cos(3*t) - (7*sin(3*t))/24 - sin(9*t)/72 - cos(3*t)*(t/6 - sin(6*t)/36)
 
